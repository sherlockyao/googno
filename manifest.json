{
  "name": "Google Quick Link",
  "version": "1.0",
  "background": { "scripts": ["background.js"] },
  "permissions": [
	  "https://*/*", "http://*/*"
  ],
  "content_scripts": [
  	  {
	    "matches": ["https://www.google.com/search*", 
			"https://www.google.com.hk/search*", 
			"http://www.google.com/search*", 
			"http://www.google.com.hk/search*"],
		"js": ["googno.js"]
	  }
  ],
  "manifest_version": 2
}
